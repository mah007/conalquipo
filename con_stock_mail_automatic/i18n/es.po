"\n"
"    <table border=\"1\">\n"
"        <thead>\n"
"            <tr>\n"
"                <th>Reference</th>\n"
"                <th>Product</th>\n"
"                <th>Origin</th>\n"
"                <th>Destination</th>\n"
"                <th>Quantity</th>\n"
"                <th>State</th>\n"
"                <th>Date expected</th>\n"
"            </tr>\n"
"        </thead>\n"
"        <tbody>\n"
