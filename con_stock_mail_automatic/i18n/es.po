"            <tr>\n"
"                <td>                                            \n"
"                    ${line['reference']}                                   \n"
"                </td>\n"
"                <td>                                            \n"
"                    ${line['name']}                                   \n"
"                </td>\n"
"                <td>                                            \n"
"                    ${line['location_id'].complete_name}    \n"
"                </td>            \n"
"                <td>                                            \n"
"                    ${line['location_dest_id'].complete_name}       \n"
"                </td>\n"
"                <td>                                            \n"
"                    ${line['product_uom_qty']}                               \n"
"                </td>\n"
"                <td>                                         \n"
"                    ${line['state']}                               \n"
"                </td>                   \n"
"                <td>   \n"
"                    ${line['date_expected']}      \n"
"                </td>            \n"
"            </tr>\n"
"        % endfor                                        \n"
"        </tbody>\n"
"    </table>\n"
"\n"
"% endif\n"
