#. module: con_stock
#: model:mail.template,body_html:con_stock.stock_automatic_email_template
msgid ""
"\n"
"\n"
"% set partners = ctx.get('partner_lst', [])\n"
"\n"
"% set workers = ctx.get('works_lst', [])\n"
"\n"
"% set moves = ctx.get('move_line_ids', [])\n"
"\n"
"% set qty_remissions = 0\n"
"\n"
"% set qty_receipts = 0\n"
"\n"
"% if partners:\n"
"    % for linep in partners:\n"
"        <table border=\"1\" width=\"800\">\n"
"            <tr>\n"
"                <th colspan=\"8\" align=\"left\" bgcolor=\"#FF9238\">${linep['name']}</th>\n"
"            </tr>\n"
"            % if workers:\n"
"                % for linew in workers:\n"
"                    % if linew['partner_id'].name == linep['name']:\n"
"                        <tr>\n"
"                            <th colspan=\"8\" bgcolor=\"#F1F1F1\">${linew['name']}</th>\n"
"                        </tr>\n"
"                        % if moves:\n"
"                            <tr>\n"
"                                <th align=\"left\" bgcolor=\"#A9A9A9\">Product</th>\n"
"                                <th align=\"left\" bgcolor=\"#A9A9A9\">Remissions</th>\n"
"                                <th align=\"left\" bgcolor=\"#A9A9A9\">Receipts</th>\n"
"                            </tr>\n"
"                            % for linem in moves:\n"
"                                % for linew in workers:\n"
"                                    % if linem['picking_id'].project_id.name == linew['name'] and linew['partner_id'].name == linep['name']:\n"
"                                        <tr>         \n"
"                                            <td>\n"
"                                                ${linem['product_id'].name}           \n"
"                                            </td>\n"
"                                            <td>\n"
"                                                % if linem['picking_id'].picking_type_id.name == 'Delivery Orders':\n"
"                                                    % set qty_remissions = qty_remissions + 1\n"
"                                                % endif\n"
"                                                ${qty_remissions} \n"
"                                            </td>          \n"
"                                            <td>\n"
"                                                % if linem['picking_id'].picking_type_id.name == 'Receipts':\n"
"                                                    % set qty_receipts = qty_receipts + 1\n"
"                                                % endif\n"
"                                                ${qty_receipts} \n"
"                                            </td>                          \n"
"                                        </tr>\n"
"                                    % endif\n"
"                                % endfor\n"
"                            % endfor\n"
"                        % endif\n"
"                    % endif\n"
"                % endfor                        \n"
"            % endif\n"
"        </table>\n"
"    % endfor\n"
"% endif\n"
"\n"
msgstr ""
"\n"
"\n"
"% set partners = ctx.get('partner_lst', [])\n"
