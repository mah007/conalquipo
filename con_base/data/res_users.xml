<?xml version="1.0"?>
<odoo>
    <data noupdate="0">

        <!-- Logistica -->
        <record id="partner_coordinador_auxlogistica" model="res.partner">
            <field name="name">Coordinar / Auxiliar logistica</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="customer" eval="False"/>
            <field name="country_id" ref="base.co"/>
        </record>

        <record id="user_logistica" model="res.users">
            <field name="partner_id" ref="con_base.partner_coordinador_auxlogistica"/>
            <field name="login">logistica</field>
            <field name="password">logistica</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="groups_id" eval="[(6,0,[ref('project.group_project_user'), ref('stock.group_stock_user')])]"/>
        </record>  

        <record id="partner_director_logistica" model="res.partner">
            <field name="name">Director Logistica</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="customer" eval="False"/>
            <field name="country_id" ref="base.co"/>
        </record>

        <record id="dir_logistica" model="res.users">
            <field name="partner_id" ref="con_base.partner_director_logistica"/>
            <field name="login">dlogistica</field>
            <field name="password">dlogistica</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="groups_id" eval="[(6,0,[ref('stock.group_stock_manager'), ref('mrp.group_mrp_manager')])]"/>
        </record>  

        <record id="partner_asiscomercial" model="res.partner">
            <field name="name">Asistente Comercial</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="customer" eval="False"/>
            <field name="country_id" ref="base.co"/>
        </record>

        <record id="acomercial" model="res.users">
            <field name="partner_id" ref="con_base.partner_asiscomercial"/>
            <field name="login">acomercial</field>
            <field name="password">acomercial</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="groups_id" eval="[(6,0,[ref('sales_team.group_sale_salesman')])]"/>
        </record>  

        <record id="partner_dcomercial" model="res.partner">
            <field name="name">Director Comercial</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="customer" eval="False"/>
            <field name="country_id" ref="base.co"/>
        </record>

        <record id="dcomercial" model="res.users">
            <field name="partner_id" ref="con_base.partner_dcomercial"/>
            <field name="login">dcomercial</field>
            <field name="password">dcomercial</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="groups_id" eval="[(6,0,[ref('sales_team.group_sale_manager')])]"/>
        </record>  

    </data>
</odoo>