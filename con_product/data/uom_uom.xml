<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="product_uom_categ_cubic" model="uom.category">
            <field name="name">Cubic</field>
        </record>

        <record id="product_uom_cubic_meter" model="uom.uom">
            <field name="name">Cubic meter (m3)</field>
            <field name="category_id" ref="product_uom_categ_cubic"/>
            <field name="factor">1.0</field>
        </record>

        <record id="product_uom_categ_energy" model="uom.category">
            <field name="name">Energy</field>
        </record>

        <record id="product_uom_fractures" model="uom.uom">
            <field name="category_id" ref="product_uom_categ_energy"/>
            <field name="name">Fracture(s)</field>
            <field name="factor" eval="1.0"/>
        </record>

    </data>
</odoo>